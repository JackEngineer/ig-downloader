{
  "name": "instagram-video-dl",
  "version": "1.0.0",
  "description": "Automated Instagram video downloader with user tracking, deduplication, and cron scheduling",
  "type": "module",
  "main": "dist/cli.js",
  "bin": {
    "ig-downloader": "./dist/cli.js",
    "igd": "./dist/cli.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/JackEngineer/ig-downloader.git"
  },
  "files": [
    "dist/",
    "README.md",
    "LICENSE"
  ],
  "engines": {
    "node": ">=16.0.0"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "start": "node dist/cli.js",
    "run-all": "node dist/cli.js run",
    "prepublishOnly": "npm run build && npm run test",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "release": "npm version patch --no-git-tag-version && npm publish --access public"
  },
  "keywords": [
    "instagram",
    "video",
    "downloader",
    "automation",
    "scraper"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "playwright": "^1.49.1"
  },
  "devDependencies": {
    "@types/node": "^20.19.32",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@vitest/coverage-v8": "^4.0.18",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "prettier": "^3.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.0",
    "vitest": "^4.0.18"
  }
}
